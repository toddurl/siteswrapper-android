    <string name="site_application_id_label">Application Id</string>
    <string name="site_application_id_tag">com.urlisit.urlesites.model.Site.applicationId</string>
    <string name="site_application_id_hint">Enter the GAE Application Id</string>
    <string name="site_application_id_description">Every application deployed to Google™ App Engine is identified by a unique application ID. You can obtain the application ID via the App Engine Administration Console.</string>
    
    <string name="site_version_label">Version</string>
    <string name="site_version_tag">com.urlisit.urlesites.model.Site.version</string>
    <string name="site_version_hint">Enter the GAE Version</string>
    <string name="site_version_description">Every application deployed to Google™ App Engine is identified by a unique version number. You can obtain the application\'s version number via the App Engine Administration Console.</string>
    
    <string name="site_default_page_label">Default Page</string>
    <string name="site_default_page_tag">com.urlisit.urlesites.model.Site.defaultPage</string>
    <string name="site_default_page_hint">Enter the default page to serve</string>
    <string name="site_default_page_description">This is the default page that users will see when they visit the site. The value of this field must match the name field of one of the pages defined for this site.‎</string>

    <string name="site_favicon_url_label">Favicon URL</string>
    <string name="site_favicon_url_tag">com.urlisit.urlesites.model.Site.faviconUrl</string>
    <string name="site_favicon_url_hint">The favicon.ico source file URL</string>
    <string name="site_favicon_url_description">The URL of a favicon.ico file to be used as the favicon for the site. The favicon is retrieved from this URL and used in the site.</string>
    
    <string name="site_web_fonts_url_label">Google™ Web Fonts URL</string>
    <string name="site_web_fonts_url_tag">com.urlisit.urlesites.model.Site.webFontsUrl</string>
    <string name="site_web_fonts_url_hint">e.g.: http://fonts.googleapis.com/css?family=Open+Sans:300,400</string>
    <string name="site_web_fonts_url_description">The URL specified in the <![CDATA[ <link href=URL /> ]]> tag generated by Google™ Web Fonts for inclusion in the site. See www/google.com/fonts for more information.</string>
    
    <string name="site_touch_icon_url_label">Apple Touch Icon URL</string>
    <string name="site_touch_icon_url_tag">com.urlisit.urlesites.model.Site.touchIconUrl</string>
    <string name="site_touch_icon_url_hint">The touch icon source file URL</string>
    <string name="site_touch_icon_url_description">The URL of a touch an Apple touch icon for use as the touch icon of the site.</string>
    
    <string name="site_logo_type_label">Display logo as:</string>
    <string name="site_logo_type_tag">com.urlisit.urlesites.model.Site.logoType</string>
    <string name="site_logo_type_hint">Html or image</string>
    <string name="site_logo_type_description">The type of logo to be used as the overall logo for the Managed Application. Must be one of; html or item.</string>
    

    <string name="site_name_tag">com.urlisit.urlesites.model.Site.name</string>
    <string name="site_name_hint">Enter the name of the website</string>
    <string name="site_name_description">This is the name which will appear at various places throughout the site, or application. For example, the name specified here will appear, as the title of the home page.</string>
    
